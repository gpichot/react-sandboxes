{
  "name": "react-sandboxes",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "sandboxes:list": "ts-node tools/sandboxes.ts",
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.14.184",
    "@types/node": "^17.0.12",
    "eslint-config-custom": "workspace:*",
    "glob": "^8.0.3",
    "prettier": "latest",
    "ts-node": "^10.9.1",
    "turbo": "latest",
    "typescript": "^4.7.4"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.9.0",
  "dependencies": {
    "chalk": "4.1.2",
    "lodash": "^4.17.21"
  }
}
